.collected-notification_-u-i {
    flex-direction: row;
    align-items: center;
    justify-content: center;
}

.white-text {
    color: white;
    -unity-text-outline: 2px black;
    -unity-font-style: bold;
    -unity-font-definition: url("project://database/Assets/Art/Fonts/Nunito-Black.ttf");
}

.medium-text {
    font-size: 48px;
}

.container{
    background-color: rgb(255, 185, 94);
    border-color: rgb(172, 123, 59);

    border-width: 2px;
    border-radius: 48px;

    flex-direction: row;
    align-items: center;
    justify-content: center;

    padding: 10px;
    padding-left: 20px;
    padding-right: 20px;
}

.icon{
    height: 64px;
    width: 64px;

    margin-left: 8px;
}